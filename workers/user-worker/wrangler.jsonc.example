{
  "name": "USER_WORKER_NAME",
  "account_id": "ACCOUNT_ID",
  "main": "src/user-worker.ts",
  "compatibility_date": "2023-03-14",
  "compatibility_flags": [
    "nodejs_compat"
  ], 
  
  "observability": { 
	"enabled": true
  },

   "kv_namespaces": [
    {
      "binding": "USER_DB",
      "id": "KV_STORE_ID"
    }
  ],

  "routes": [
    {
      "pattern": "USER_WORKER_DOMAIN",
      "custom_domain": true
    }
  ],

  "placement": { "mode": "smart" }
}